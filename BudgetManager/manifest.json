{
    "manifest_version": 3,
    "name": "Budget Manager 2",
    "description": "This extension tracks overall spendings",
    "version": "1.0",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },

    "action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },

    "background": {
        "service_worker": "eventPage.js"
    },

    "permissions": [
        "storage",
        "notifications",
        "contextMenus"
    ],

    "options_page": "options.html"
}